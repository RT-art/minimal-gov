{
    "family": "workload-test-app-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::653502182074:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "app",
        "image": "653502182074.dkr.ecr.ap-northeast-1.amazonaws.com/workload-test-app-app:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "environment": [
          {
            "name": "ENV",
            "value": "dev"
          },
          {
            "name": "PORT",
            "value": "8080"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/workload-test-app",
            "awslogs-region": "ap-northeast-1",
            "awslogs-stream-prefix": "app"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
  